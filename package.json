{
  "name": "iobroker.sourceanalytix",
  "version": "0.4.14",
  "description": "Detailed statistics of your energy, gas and liquid consumption. Allows annual report and details according to weekly, monthly and quarterly consumption data.",
  "author": {
    "name": "DutchmanNL",
    "email": "oss@DrozmotiX.eu"
  },
  "contributors": [
    {
      "name": "xXBJXx"
    }
  ],
  "homepage": "https://github.com/DrozmotiX/ioBroker.sourceanalytix",
  "license": "MIT",
  "keywords": [
    "energy",
    "power",
    "analytics",
    "consumption",
    "measurement",
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DrozmotiX/ioBroker.sourceanalytix"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.0",
    "cron": "^2.0.0",
    "iobroker-adapter-helpers": "0.0.8"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@alcalzone/release-script-plugin-iobroker": "3.5.9",
    "@alcalzone/release-script-plugin-license": "3.5.9",
    "@iobroker/adapter-dev": "^1.0.1",
    "@iobroker/adapter-react": "2.2.4",
    "@iobroker/testing": "^3.0.2",
    "@material-ui/core": "^4.12.4",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.11",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^17.0.17",
    "@types/react": "^17.0.45",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "proxyquire": "^2.1.3",
    "react-dom": "^17.0.2",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit --jsx react",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "translate": "translate-adapter",
    "release": "release-script",
    "devServerStart": "dev-server watch --noStart"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/DrozmotiX/ioBroker.sourceanalytix/issues"
  },
  "readmeFilename": "README.md"
}
